    <PackageId>CafeBaazarApi</PackageId>
    <Authors>Mohammad Abolhasani</Authors>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageIcon>cafebaazar.png</PackageIcon>
    <PackageTags>git</PackageTags>
